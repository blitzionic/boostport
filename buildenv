export ZOPEN_STABLE_URL="https://github.com/boostorg/boost.git"
export ZOPEN_STABLE_DEPS="cmake zoslib"
export ZOPEN_DEV_URL="https://github.com/boostorg/boost.git"
export ZOPEN_DEV_DEPS="cmake zoslib"
export ZOPEN_BUILD_LINE="STABLE"

export ZOPEN_CONFIGURE="./libs/config"
export ZOPEN_CONFIGURE_OPTIS="./b2 toolset=clang"
zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to gauge build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
  echo "expectedTotalTests:1"
}

